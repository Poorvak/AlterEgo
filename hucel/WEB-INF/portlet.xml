<portlet-app
    xmlns="http://java.sun.com/xml/ns/portlet/portlet-app_1_0.xsd"
    version="1.0"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://java.sun.com/xml/ns/portlet/portlet-app_1_0.xsd
                        http://java.sun.com/xml/ns/portlet/portlet-app_1_0.xsd">


<portlet>
    <portlet-name>100</portlet-name>
    <display-name>CohsePortlet</display-name>
    <portlet-class>uk.ac.man.cs.img.cohse.portlet.CohsePortlet</portlet-class>
    <expiration-cache>0</expiration-cache>
    <supports>
        <mime-type>text/html</mime-type>
        <portlet-mode>view</portlet-mode>
        <portlet-mode>edit</portlet-mode>
        <portlet-mode>help</portlet-mode>
    </supports>
    <supported-locale>en_US</supported-locale>
     <portlet-info>
        <title>Cohse Portlet</title>
        <short-title>Cohse Portlet</short-title>
        <keywords>Cohse Portlet</keywords>
    </portlet-info>
    <portlet-preferences>
        <preferences-validator>uk.ac.man.cs.img.cohse.portlet.CohsePreferencesValidator</preferences-validator>
    </portlet-preferences>
  </portlet>  
</portlet-app>
